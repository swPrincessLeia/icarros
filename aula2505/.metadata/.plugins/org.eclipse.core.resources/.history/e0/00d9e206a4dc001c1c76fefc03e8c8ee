
public class Agenda {

	public static void main(String[] args) {

		ListaDeContatos sintaxeBanco = new ListaDeContatos();
		BuscaDeContatos buscaContatos = new BuscaDeContatos();	
		
		sintaxeBanco.Conectar();
		buscaContatos.Conectar();

		if (sintaxeBanco.estaConectado()) {
			System.out.println("Conectou! :D");
			sintaxeBanco.listaContatos();
			buscaContatos.buscaContatos();

		} else {

			System.out.println("Deu ruim! :/");
		}
	}

}
